      <div class="mx-auto" >
       <img class="blink-image"  </img>
      </div>
  </div>
</section>

<!-- Projects Section -->
<section id="projects" class="projects-section bg-dark">
<div class="container">
  <div class="album text-muted bg-dark" style="padding:0px;">
     <p class="text-white display-3 blink-image text-center">진무사 추천 도장</p>
     <div class="container">
        <div class="row bg-dark">
           <% @posts.each do |product| %>
           <div class="card2">
              <%= image_tag product.image, :style => "height: 280px; width: 100%;  display: block;" , :class => "card-img-top" %>
              <strong class="card2-text"><%= product.CourtName %></strong>
              <p class="card2-text card-text"><%= product.Created %></p>
              <p class="card2-text"><%= product.CourtSubIntro %></p>
              <a href="<%= edit_post_path(product) %>" class="btn btn-primary">Go somewhere</a>
           </div>
           <% end %>
        </div>
     </div>
  </div>
    <!-- Project One Row -->
    <div class="row justify-content-center no-gutters mb-5 mb-lg-0">
      <div class="col-lg-6">
        <img class="blink-image img-fluid" style='height: 100%; width: 100%; object-fit: contain' src="https://encrypted-tbn0.gstatic.com/images?q=tbn%3AANd9GcSNcIAaIqjTsy2ZS1a8QsLeHaIhILGN6PahQcVhkje8nmRRlHrB&usqp=CAU">
        </div>
      <div class="col-lg-6">
        <div class="bg-black text-center h-100 project">
          <div class="d-flex h-100">
            <div class="project-text w-100 my-auto text-center text-lg-left">
              <h4 class="text-white">최우수 도장</h4>
              <p class="mb-0 text-white-50">An example of where you can put an image of a project, or anything else, along with a description.</p>
              <hr class="d-none d-lg-block mb-0 ml-0">
            </div>
          </div>
        </div>
      </div>
    </div>

    <!-- Project Two Row -->
    <div class="row justify-content-center no-gutters">
      <div class="col-lg-6">
        <img class="blink-image img-fluid" style='height: 100%; width: 100%; object-fit: contain' src="data:image/jpeg;base64," alt="">
      </div>
      <div class="col-lg-6 order-lg-first">
        <div class="bg-black text-center h-100 project">
          <div class="d-flex h-100">
            <div class="project-text w-100 my-auto text-center text-lg-right">
              <h4 class="text-white">우수 도장</h4>
              <p class="mb-0 text-white-50">Another example of a project with its respective description. These sections work well responsively as well, try this theme on a small screen!</p>
              <hr class="d-none d-lg-block mb-0 mr-0">
            </div>
          </div>
        </div>
      </div>
    </div>

  </div>
</section>

<!-- Signup Section -->
<section id="signup" class="signup-section">
  <div class="container">
    <div class="row">
      <div class="col-md-10 col-lg-8 mx-auto text-center">

        <i class="far fa-paper-plane fa-2x mb-2 text-white width=10;"></i>
        <h2 class="text-white mb-5 blink-image">알맞은 도장을 검색하세요!</h2>


        <div class="form-inline d-flex">
           <input type="text" class="form-control flex-fill mr-0 mr-sm-2 mb-3 mb-sm-0" id="SearchInput"  placeholder="도장을 검색하세요...">
           
            <!-- <div class="btn btn-primary mx-auto" ><%= link_to '검색', posts_path(:searchtext => "SearchText.text") %> </div> -->
            
            <button type="submit" class="btn btn-primary mx-auto" id="SearchText">검색</button>
            <div class="btn-group" role="group" aria-label="Basic example">
              <button id="CategoryID1" type="button" class="btn btn-secondary">최신순</button>
              <button id="CategoryID2" type="button" class="btn btn-secondary">거리순</button>
              <button id="CategoryID3" type="button" class="btn btn-secondary">평점순</button>
            </div>

        </div>

      </div>
    </div>
  </div>
</section>

<!-- Contact Section -->
